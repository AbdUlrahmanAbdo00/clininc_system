{
  "version": 2,
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.7.3"
    }
  },
  "builds": [
    {
      "src": "**/*.php",
      "use": "@vercel/php",
      "config": {
        "phpVersion": "8.2",
        "includePaths": ["**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ]
}